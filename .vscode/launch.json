{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch a test",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${file}",
            "args": [
                "-test.v",
                "-test.run",
                "^${selectedText}$"
            ],
            "env": {
                "TF_ACC": "1"
            },
            "buildFlags": "-v -tags=all",
            "showLog": true,
            //"envFile": "${workspaceFolder}/.env"
        }
    ]
}
